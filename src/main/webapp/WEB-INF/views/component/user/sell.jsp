    <div class="form-group">
        <form:form modelAttribute="sellRegisterDTO" cssClass="mb-5" action="" method="POST">
            <%-- tên cửa hàng --%>
            <div class="form-floating mb-3">
                <form:input path="name" cssClass="form-control" id="name" placeholder="Tên cửa hàng" />
                <form:label path="name" for="name">Tên cửa hàng</form:label>
                <form:errors path="name" cssClass="text-danger" />
            </div>
            <%-- mô tả --%>
            <div class="form-floating mb-3">
                <form:textarea path="description" cssClass="form-control" id="description" placeholder="Mô tả cửa hàng" style="height: 100px" />
                <form:label path="description" for="description">Mô tả cửa hàng</form:label>
                <form:errors path="description" cssClass="text-danger" />
            </div>



            <%-- địa chỉ --%>
            <div class="row">
                <div class="col-md-4">
                    <%-- provinceId --%>
                    <div class="form-floating mb-3">
                        <form:select path="provinceId" cssClass="form-select" id="provinceId">
                            <form:option value="" label="Chọn tỉnh thành" />
                            <form:options items="${provinces}" itemValue="id" itemLabel="name" />
                        </form:select>
                        <form:label path="provinceId" for="provinceId">Tỉnh thành</form:label>
                        <form:errors path="provinceId" cssClass="text-danger" />
                    </div>
                </div>
                <div class="col-md-4">
                    <%-- districtId --%>
                    <div class="form-floating mb-3">
                        <form:select path="districtId" cssClass="form-select" id="districtId">
                            <form:option value="" label="Chọn quận huyện" />
                        </form:select>
                        <form:label path="districtId" for="districtId">Quận huyện</form:label>
                        <form:errors path="districtId" cssClass="text-danger" />
                    </div>
                </div>
                <div class="col-md-4">
                    <%-- wardId --%>
                    <div class="form-floating mb-3">
                        <form:select path="wardId" cssClass="form-select" id="wardId">
                            <form:option value="" label="Chọn phường xã" />
                        </form:select>
                        <form:label path="wardId" for="wardId">Phường xã</form:label>
                        <form:errors path="wardId" cssClass="text-danger" />
                    </div>
                </div>
                <div class="col-12">
                    <%-- detail address --%>
                    <div class="form-floating mb-3">
                        <form:input path="detailAddress" cssClass="form-control" id="detailAddress" placeholder="Địa chỉ cụ thể" />
                        <form:label path="detailAddress" for="detailAddress">Địa chỉ chi tiết</form:label>
                        <form:errors path="detailAddress" cssClass="text-danger" />
                    </div>
                </div>
                <div class="col-12">
                    <%-- full address --%>
                    <div class="form-floating mb-3">
                        <input type="text" class="form-control" id="fullAddress" readonly disabled />
                        <label for="fullAddress">Địa chỉ đầy đủ</label>
                    </div>
                </div>
            </div>

            <%-- chấp nhận điều khoản --%>
            <p class="text-danger">* Bằng việc bạn tick vào ô bên dưới, bạn đã đồng ý với các điều khoản sử dụng dịch vụ bán hàng của chúng tôi</p>
            <div class="form-check mb-3">
                <form:checkbox path="acceptData" id="acceptData" cssClass="form-check-input" />
                <form:label path="acceptData" cssClass="form-check" for="acceptData">Tôi đồng ý với các điều khoản sử dụng dịch vụ bán hàng</form:label>
                <form:errors path="acceptData" cssClass="text-danger" />
            </div>
            <button type="submit" class="btn btn-primary mt-3">Đăng ký</button>

        </form:form>
    </div>


</div>
